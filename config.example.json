{
    "website": "https://current-hosted-site-for-autopush.org",
    "IPsAllowList": [
        "X.Y.Z.W"
    ],
    "SSHKeysPath": "/home/myusername/.ssh",
    "ReposBasePath": "/var/www",
    "CustomCommands": {
        "repo_name": [
            "ls /home",
            "cd /home/$ReposBasePath/$repo",
            "GIT_SSH_COMMAND=\"ssh -i $SSHKeysPath/$key \" git pull"
        ]
    }
}
